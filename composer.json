{
	"name": "nielslange/smntcs-image-dimensions",
	"description": "Shows the image dimension and the image file size in the media library.",
	"homepage": "https://github.com/nielslange/smntcs-image-dimensions",
	"support": {
		"issues": "https://github.com/nielslange/smntcs-image-dimensions/issues",
		"source": "https://github.com/nielslange/smntcs-image-dimensions"
	},
	"type": "wordpress-plugin",
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Image Dimensions"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Niels Lange",
			"email": "info@nielslange.de"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"overtrue/phplint": "^3.0",
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"squizlabs/php_codesniffer": "^3.6",
		"wp-coding-standards/wpcs": "^2.3"
	},
	"scripts": {
		"phpcbf": "phpcbf --standard=phpcs.dist.xml .",
		"phpcs": "phpcs --standard=phpcs.dist.xml .",
		"phplint": "phplint"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
